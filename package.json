{
  "name": "@junduck/trading-indi",
  "version": "0.999.0",
  "description": "A TypeScript library providing 80+ technical indicators for trading, designed for incremental, stateful calculation perfect for intraday and realtime trading applications",
  "keywords": [
    "trading",
    "algorithmic-trading",
    "technical-indicators",
    "real-time",
    "technical-analysis"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./indicators": {
      "types": "./dist/indicators/index.d.ts",
      "import": "./dist/indicators/index.js"
    },
    "./primitive": {
      "types": "./dist/primitive/index.d.ts",
      "import": "./dist/primitive/index.js"
    },
    "./aggregation": {
      "types": "./dist/aggregation/index.d.ts",
      "import": "./dist/aggregation/index.js"
    },
    "./heuristics": {
      "types": "./dist/heuristics/index.d.ts",
      "import": "./dist/heuristics/index.js"
    },
    "./flow": {
      "types": "./dist/flow/index.d.ts",
      "import": "./dist/flow/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "bench:graph": "NODE_OPTIONS=\"--expose-gc\" tsx benchmarks/graph-performance.ts",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "author": "Jun",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/junduck/trading-indi.git"
  },
  "bugs": {
    "url": "https://github.com/junduck/trading-indi/issues"
  },
  "homepage": "https://github.com/junduck/trading-indi",
  "packageManager": "pnpm@10.21.0",
  "devDependencies": {
    "@ai-sdk/anthropic": "^2.0.45",
    "@ai-sdk/openai": "^2.0.71",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "ai": "^5.0.100",
    "autocannon": "^8.0.0",
    "clinic": "^13.0.0",
    "dotenv": "^17.2.3",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  },
  "dependencies": {
    "@junduck/trading-core": "^2.9.0",
    "zod": "^4.1.12"
  }
}
